<form action="{{ rootFolder }}cruds/createAccount.php"
  method="post"
  class="login">
  <input type="text"
    placeholder="Username"
    name="account[username]"
    class="login__field form-control"
    required />
  <input type="password"
    placeholder="password"
    name="account[password]"
    class="login__field form-control"
    required />
  <input type="submit" value="Registreer" class="btn btn-warning" />
</form>
